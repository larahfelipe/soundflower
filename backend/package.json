{
  "name": "soundflower-api",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/larafe1/soundflower.git",
  "author": "larafe1",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/app.js",
    "dev": "tsnd --respawn --transpile-only --exit-child -r tsconfig-paths/register src/app.ts",
    "build": "tsc --project tsconfig.json&&tscpaths -p tsconfig.json -s src -o dist",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --check \"src/**/*.ts\" --write",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.1",
    "@vercel/node": "^2.6.1",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "fastify": "^4.9.2",
    "fastify-zod": "^1.2.0",
    "ioredis": "^5.2.4",
    "node-vibrant": "^3.2.1-alpha.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "yt-search": "^2.10.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
    "@types/yt-search": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
